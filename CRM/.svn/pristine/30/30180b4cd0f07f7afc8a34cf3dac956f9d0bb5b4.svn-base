/* 日历 */
.calendar-box{
	position: relative;
	height: 380px;
	width: 300px;
	bottom: 0;
	margin-left: 4%;
    display: block;
}
.calendar{
	position: absolute;
	width: 100%;
	height: 100%;
	perspective:1000px;
}
.header{
	position: absolute;
	height: 100px;
	width: 100%;
	background-color: #98c2c2;
	transform-origin: bottom;
	transform: rotateX(90deg);
	backface-visibility: hidden;
	transition:.4s ease-in-out 0s;
	box-shadow:0 6px 20px 0 rgba(0,0,0,.19), 0 8px 17px 0 rgba(0,0,0,.2);
}
.prev-month,.next-month{
	position: absolute;
	width: 40px;
    height: 40px;
    top: 50%;
    margin-top: -20px;
    z-index: 1;
    font-size: 35px;
    text-align: center;
    color: white;
    z-index: 999;
}
.prev-month:hover,.next-month:hover,.prev-month:link,.next-month:link,.prev-month:visited,.next-month:visited{
	color: white;
}
.prev-month{
    left: 15px;
}
.text{
	height: 70px;
	width: 270px;
	position: absolute;
	top: 15px;
	left: 15px;
	font-size: 20px;
    text-align: center;
    color: white;
    line-height: 70px;
    font-size: 100%;
    
}
.next-month{
	right: 15px;
}
.months{
	position: absolute;
	height: calc(100% - 100px);
	width: 100%;
	bottom: 0;
	background-color: white;
	transform-origin: bottom;
	transform: rotateY(360deg);
	backface-visibility: hidden;
	transition:.4s ease-in-out 0s;
	box-shadow:0 6px 20px 0 rgba(0,0,0,.19), 0 8px 17px 0 rgba(0,0,0,.2);
}
.month{
	position: absolute;
    width: 270px;
    height: 250px;
    top: 15px;
    left: 15px;
    display: block;
}
.render{
	position: absolute;
    width: 100%;
    height: 100%;
    top: 0;
    left: 0;
    display: block;
}
.calendar_table{
	position: absolute;
    width: 100%;
    height: 100%;
    top: 0;
    left: 0;
    display: block;
    font-size: 10px;
    font-weight: 400;
}
.weeks{
    width: 100%;
    height: 28px;
    top: 0;
    left: 0;
    color: #98c2c2;
}
.cell-week{
	width: 14.28%;
	height: 28px;
    text-align: center;
    line-height: 28px;
    float: left;
}
.calendar_row{
    width: 100%;
    height: 37px;
    top: 0;
    left: 0;
}
.cell{
	width: 14.28%;
	height: 37px;
    text-align: center;
    line-height: 37px;
    float: left;
}
.noCurrentMonth{
	color: #C0C0C0;
}
.currentMonth{
	color: #98c2c2;
}
.currentDay{
	background-color: #98c2c2;
	color: white;
}
@keyframes render{
	0%   {transform: rotateX(0deg);}
	50%  {transform: rotateX(45deg);}
	100% {transform: rotateX(0deg);}
}


